{
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "init"]
    },
    "build:mac-universal": {
      "dependsOn": ["^build", "init"]
    },
    "init": {
      "dependsOn": ["^build", "^pack-for-forge-npm-install"]
    },
    "pack-for-forge-npm-install": {
      "dependsOn": ["build"]
    },
    "test:e2e": {
      "dependsOn": ["build"]
    },
    "test:e2e:ci-linux": {
      "dependsOn": ["build"]
    },
    "test:e2e:mac-universal": {
      "dependsOn": ["build:mac-universal"]
    },
    "test:unit": {
      "dependsOn": ["^build"]
    }
  },
  "targets": {
    "clean": {
      "cache": false
    },
    "clean:dist": {
      "cache": false
    },
    "exec": {},
    "exec:ci-linux": {},
    "release": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {},
    "test:dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    }
  }
}
