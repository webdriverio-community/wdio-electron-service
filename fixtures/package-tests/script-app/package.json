{
  "name": "script-app-example",
  "version": "9.0.0-next.0",
  "description": "Simple Electron app example using wdio-electron-service",
  "main": "out/main/index.js",
  "type": "module",
  "scripts": {
    "clean": "pnpx shx rm -rf out node_modules",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron .",
    "test": "cross-env DEBUG=wdio-electron-service xvfb-maybe wdio run wdio.conf.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@wdio/cli": "^9.16.2",
    "@wdio/globals": "^9.16.2",
    "@wdio/local-runner": "^9.16.2",
    "@wdio/mocha-framework": "^9.16.2",
    "@wdio/spec-reporter": "^9.16.2",
    "cross-env": "^7.0.3",
    "electron": "^37.1.0",
    "electron-vite": "beta",
    "typescript": "^5.8.3",
    "wdio-electron-service": "latest",
    "xvfb-maybe": "^0.2.1"
  },
  "keywords": [
    "electron",
    "electron-vite",
    "webdriverio",
    "testing",
    "example"
  ],
  "author": "WebDriverIO Community",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  }
