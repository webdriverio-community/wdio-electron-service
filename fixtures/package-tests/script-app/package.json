{
  "name": "script-app-example",
  "version": "0.0.0",
  "description": "Simple Electron app example using wdio-electron-service",
  "main": "out/main/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "pnpx shx rm -rf out node_modules",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron .",
    "test": "cross-env DEBUG=wdio-electron-service wdio run wdio.conf.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.9.0",
    "@types/semver": "^7.7.1",
    "@wdio/cli": "^9.20.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "cross-env": "^10.1.0",
    "electron": "38.2.2",
    "electron-vite": "^4.0.1",
    "semver": "^7.7.3",
    "typescript": "^5.9.3",
    "wdio-electron-service": "link:packages/wdio-electron-service"
  },
  "keywords": ["electron", "electron-vite", "webdriverio", "testing", "example"],
  "author": "WebDriverIO Community",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": ["electron"]
