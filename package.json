{
  "name": "wdio-electron-service-monorepo",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18 || >=20"
  },
  "scripts": {
    "build": "turbo run build --filter=@wdio* --filter=wdio-electron-service --only --force",
    "backport": "tsx ./scripts/backport.ts",
    "catalog:default": "tsx ./scripts/switch-catalog.ts default",
    "catalog:next": "tsx ./scripts/switch-catalog.ts next",
    "catalog:minimum": "tsx ./scripts/switch-catalog.ts minimum",
    "catalog:update": "tsx ./scripts/update-catalogs.ts",
    "catalog:update:dry": "tsx ./scripts/update-catalogs.ts --dry-run",
    "ci": "turbo run lint format:check test:unit test:integration test:package test:e2e:builder-cjs test:e2e:builder-esm test:e2e:forge-cjs test:e2e:forge-esm test:e2e:no-binary-cjs test:e2e:no-binary-esm",
    "ci:e2e:clean": "turbo run clean --filter=example*",
    "ci:e2e:logs": "turbo run logs --filter=@repo/e2e",
    "clean": "turbo run clean && pnpm clean:cache && pnpm dlx shx rm -rf node_modules",
    "clean:cache": "turbo daemon stop && pnpm dlx shx rm -rf .turbo .eslintcache",
    "format": "biome format --write .",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false .",
    "graph": "pnpm graph:e2e && pnpm graph:e2e:mac-universal",
    "graph:e2e": "tsx ./scripts/create-task-graph.ts ./.github/assets/e2e-graph.png test:e2e:forge-cjs test:e2e:forge-esm test:e2e:builder-cjs test:e2e:builder-esm test:e2e:no-binary-cjs test:e2e:no-binary-esm",
    "graph:e2e:mac-universal": "tsx ./scripts/create-task-graph.ts ./.github/assets/e2e-graph-mac-universal.png test:e2e-mac-universal:builder-cjs test:e2e-mac-universal:builder-esm test:e2e-mac-universal:forge-cjs test:e2e-mac-universal:forge-esm",
    "lint": "biome check --formatter-enabled=false --linter-enabled=true . && cross-env eslint --cache \"**/*.{j,mj,cj,t}s\"",
    "lint:fix": "biome check --write --formatter-enabled=false --linter-enabled=true . && cross-env eslint --cache --fix \"**/*.{j,mj,cj,t}s\"",
    "lint:fix:unsafe": "biome check --write --formatter-enabled=false --linter-enabled=true --unsafe . && cross-env eslint --cache --fix \"**/*.{j,mj,cj,t}s\"",
    "prepare": "husky",
    "release": "turbo run release --filter=wdio-electron-service --filter=@wdio* --only --concurrency=1",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test:unit test:integration test:package",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:package": "pnpm dlx cross-env DEBUG=wdio-electron-service tsx ./scripts/test-package.ts",
    "test:e2e-local": "pnpm dlx cross-env DEBUG=wdio-electron-service pnpm clean && pnpm i && pnpm build && pnpm run ci",
    "test:e2e-mac-universal-local": "pnpm dlx cross-env DEBUG=wdio-electron-service pnpm clean && pnpm i && turbo run build:mac-universal init-e2es test:e2e-mac-universal:builder-cjs test:e2e-mac-universal:builder-esm test:e2e-mac-universal:forge-cjs test:e2e-mac-universal:forge-esm --only",
    "e2e": "pnpm --filter @repo/e2e run test",
    "e2e:builder": "pnpm --filter @repo/e2e run test:e2e:builder",
    "e2e:forge": "pnpm --filter @repo/e2e run test:e2e:forge",
    "e2e:no-binary": "pnpm --filter @repo/e2e run test:e2e:no-binary",
    "e2e:cjs": "pnpm --filter @repo/e2e run test:e2e:cjs",
    "e2e:esm": "pnpm --filter @repo/e2e run test:e2e:esm",
    "e2e:window": "pnpm --filter @repo/e2e run test:e2e:window",
    "e2e:multiremote": "pnpm --filter @repo/e2e run test:e2e:multiremote",
    "e2e:standalone": "pnpm --filter @repo/e2e run test:e2e:standalone",
    "update:all": "pnpm catalog:update --default && pnpm catalog:update --next && pnpm up -iLr"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@inquirer/prompts": "^7.8.3",
    "@octokit/rest": "^22.0.0",
    "@octokit/types": "^14.1.0",
    "@types/node": "^24.3.0",
    "@types/shelljs": "^0.8.17",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "chalk": "^5.6.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-plugin-wdio": "^9.16.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "list": "^2.0.19",
    "ora": "^8.2.0",
    "package-versioner": "^0.8.6",
    "read-package-up": "^11.0.0",
    "rollup": "^4.48.1",
    "shelljs": "^0.10.0",
    "shx": "^0.4.0",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "yaml": "^2.8.1"
  },
  "lint-staged": {
    "**/*.{j,t}s": ["biome check --write", "eslint --fix --cache"],
    "**/*.{yml,md,json}": ["biome format --write ."]
  },
  "pnpm": {
    "onlyBuiltDependencies": ["edgedriver", "electron", "esbuild", "geckodriver"],
    "overrides": {
      "wdio-electron-service": "link:packages/wdio-electron-service"
    }
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "version": "9.1.1"
}
