{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "//#format:check": {
      "inputs": ["**/*.{j,t}s", "**/*.{yml,md,json}"]
    },
    "//#lint": {
      "inputs": ["**/*.{j,mj,cj,t}s"]
    },
    "@wdio/electron-bundler#build": {
      "dependsOn": ["test:unit", "typecheck"],
      "outputs": ["dist/**"]
    },
    "@wdio/electron-bundler#release": {
      "dependsOn": ["build"],
      "cache": false,
      "env": ["GITHUB_TOKEN", "NODE_AUTH_TOKEN"]
    },
    "@wdio/electron-types#build": {
      "dependsOn": ["@wdio/electron-bundler#build", "test:unit", "typecheck"],
      "outputs": ["dist/**"]
    },
    "@wdio/electron-types#release": {
      "dependsOn": ["build"],
      "cache": false,
      "env": ["GITHUB_TOKEN", "NODE_AUTH_TOKEN"]
    },
    "@wdio/electron-utils#build": {
      "dependsOn": ["@wdio/electron-types#build", "test:unit", "typecheck"],
      "outputs": ["dist/**"]
    },
    "@wdio/electron-utils#release": {
      "dependsOn": ["build"],
      "cache": false,
      "env": ["GITHUB_TOKEN", "NODE_AUTH_TOKEN"]
    },
    "clean": {
      "cache": false
    },
    "clean:dist": {
      "cache": false
    },
    "example-builder-cjs#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-builder-esm#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-forge-cjs#build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "example-forge-esm#build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "example-no-binary-cjs#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-no-binary-esm#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-builder-cjs#build:mac-universal": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-builder-esm#build:mac-universal": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "example-forge-cjs#build:mac-universal": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "example-forge-esm#build:mac-universal": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "persistent": true
    },
    "test": {},
    "test:unit": {
      "dependsOn": ["^build"]
    },
    "wdio-electron-service#build": {
      "dependsOn": ["@wdio/electron-utils#build", "test:unit", "typecheck"],
      "outputs": ["dist/**"]
    },
    "wdio-electron-service#release": {
      "dependsOn": ["build"],
      "cache": false,
      "env": ["GITHUB_TOKEN", "NODE_AUTH_TOKEN"]
    },
    "@repo/e2e#test:all": {
      "dependsOn": ["^build"]
    },
    "@repo/e2e#test:standard": {
      "dependsOn": ["^build"]
    },
    "@repo/e2e#test:window": {
      "dependsOn": ["^build"]
    },
    "@repo/e2e#test:multiremote": {
      "dependsOn": ["^build"]
    },
    "@repo/e2e#test:standalone": {
      "dependsOn": ["^build"]
    },
    "@repo/e2e#test:builder:cjs": {
      "dependsOn": ["example-builder-cjs#build"]
    },
    "@repo/e2e#test:builder:esm": {
      "dependsOn": ["example-builder-esm#build"]
    },
    "@repo/e2e#test:forge:cjs": {
      "dependsOn": ["example-forge-cjs#build"]
    },
    "@repo/e2e#test:forge:esm": {
      "dependsOn": ["example-forge-esm#build"]
    },
    "@repo/e2e#test:no-binary:cjs": {
      "dependsOn": ["example-no-binary-cjs#build"]
    },
    "@repo/e2e#test:no-binary:esm": {
      "dependsOn": ["example-no-binary-esm#build"]
    }
  }
}
