{
  "name": "wdio-electron-service-example-cjs",
  "version": "1.0.0",
  "main": "dist/main.bundle.js",
  "scripts": {
    "build": "pnpm build:bundle && pnpm build:package",
    "build:bundle": "tsc && rollup -c rollup.config.mjs",
    "build:package": "cp ./src/index.html ./dist && electron-forge package",
    "ci": "pnpm i && pnpm build && pnpm test",
    "clean": "pnpm clean:dist && rm -rf ./node_modules pnpm-lock.yaml ./all-logs",
    "clean:dist": "pnpx rimraf ./dist && mkdir -p ./dist",
    "test": "wdio run ./wdio.conf.ts"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/core": "^6.4.2",
    "@electron-forge/core-utils": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/shared-types": "^6.4.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^20.8.9",
    "@wdio/cli": "^8.20.5",
    "@wdio/globals": "^8.20.5",
    "@wdio/local-runner": "^8.20.5",
    "@wdio/mocha-framework": "^8.20.3",
    "electron": "^27.0.2",
    "global-jsdom": "^9.1.0",
    "jsdom": "^22.1.0",
    "rollup": "^4.1.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wdio-electron-service": "file:../",
    "webdriverio": "^8.20.4"
  },
  "peerDependencies": {
    "typescript": "5.2.2"
  }
}
