{
  "name": "@wdio/electron-types",
  "version": "7.0.2",
  "description": "Types for WebdriverIO Electron Service",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "pnpm clean:dist && pnpm dlx shx rm -rf ./node_modules pnpm-lock.yaml",
    "clean:dist": "pnpm dlx shx rm -rf ./dist",
    "build": "tsc",
    "release": "release-it --ci --npm.skipChecks --no-git.requireCleanWorkingDir"
  },
  "dependencies": {
    "@vitest/spy": "^2.1.1"
  },
  "devDependencies": {
    "@electron-forge/shared-types": "^7.4.0",
    "@electron/packager": "^18.3.5",
    "@wdio/globals": "^9.0.9",
    "@wdio/types": "^9.0.8",
    "builder-util": "25.0.3",
    "electron": "^32.1.2",
    "read-package-up": "^11.0.0",
    "release-it": "^17.6.0",
    "typescript": "^5.6.2",
    "webdriverio": "^9.0.9"
  },
  "files": [
    "dist/*"
  ],
  "release-it": {
    "hooks": {
      "after:bump": "pnpm pack"
    },
    "git": false,
    "npm": {
      "publish": true,
      "publishPath": "*.tgz"
    }
  }
}
